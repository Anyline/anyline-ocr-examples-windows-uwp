{
  "captureResolution": "2448",
  "resolutionRatio": {
    "numerator": 4,
    "denominator": 3
  },
  "cutout": {
    "style": "rect",
    "width": 1200,
    "maxWidthPercent": "75%",
    "maxHeightPercent": "75%",
    "alignment": "top_half",
    "image": "Overlay",
    "ratioFromSize": {
      "width": 1,
      "height": 1
    },
    "strokeWidth": 2,
    "strokeColor": "FFFFFF",
    "cornerRadius": 4,
    "offset": {
      "x": 0,
      "y": 0
    },
    "cropPadding": {
      "x": 0,
      "y": 0
    },
    "cropOffset": {
      "x": 0,
      "y": 0
    },
    "outerColor": "000000",
    "outerAlpha": 0.3
  },
  "flash": {
    "mode": "auto",
    "alignment": "bottom_right",
    "imageOn": "torch_on",
    "imageOff": "torch_off",
    "imageAuto": "torch_auto",
    "offset": {
      "x": 0,
      "y": 0
    }
  },
  "beepOnResult": false,
  "vibrateOnResult": true,
  "blinkAnimationOnResult": true,
  "cancelOnResult": true,
  "visualFeedback": {
    "style": "RECT",
    "strokeColor": "0099FF",
    "fillColor": "220099FF",
    "animationDuration": 70
  }
}